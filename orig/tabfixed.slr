#!/bin/bash
#SBATCH -J tabfixed
#SBATCH --account=rogersa-kp
#SBATCH --partition=rogersa-kp
#SBATCH --time=10:00:00
#SBATCH --nodes 1
#SBATCH --ntasks 1
#SBATCH -o tabfixed.txt # stdout
#SBATCH -e tabfixed.err # stderr

ls *hg19*.mod-fixed.vcf.gz | xargs -n 1 bcftools query -f '[%GT ]\n' \
  | awk -f tabgtype.awk
